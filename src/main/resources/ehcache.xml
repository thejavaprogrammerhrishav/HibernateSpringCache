<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ehcache>
<ehcache>
    <diskStore path="java.io.tmpdir"/>
    <defaultCache  maxElementsInMemory="90000" eternal="false" timeToIdleSeconds="3600"
                   timeToLiveSeconds="3600" overflowToDisk="true"/>
    <cache name="ehcache"
           maxElementsInMemory="90000" eternal="false" timeToIdleSeconds="3600"
           timeToLiveSeconds="3600" overflowToDisk="true"/>
</ehcache>